@Article{Agirre23,
  author    = {Agirre, J and Atanasova, M and Bagdonas, H and Ballard, CB and Basl{\'e}, A and Beilsten-Edmands, J and Borges, RJ and Brown, DG and Burgos-M{\'a}rmol, JJ and Berrisford, JM and others},
