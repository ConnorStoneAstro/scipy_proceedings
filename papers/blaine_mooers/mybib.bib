  keywords  = {Collaborative Computational Project No. 4; CCP4; crystallography software; macromolecular crystallography},
  publisher = {International Union of Crystallography},
  year      = {2023},
}

@Article{McNicholas18,
  author    = {McNicholas, Stuart and Croll, Tristan and Burnley, Tom and Palmer, Colin M and Hoh, Soon Wen and Jenkins, Huw T and Dodson, Eleanor and Cowtan, Kevin and Agirre, Jon},
  title     = {Automating tasks in protein structure determination with the clipper python module},
  number    = {1},
  pages     = {207--216},
  volume    = {27},
  abstract  = {Scripting programming languages provide the fastest means of prototyping complexfunctionality. Those with a syntax and grammar resembling human language also greatly enhance themaintainability of the produced source code. Furthermore, the combination of a powerful, machine-independent scripting language with binary libraries tailored for each computer architecture allowsprograms to break free from the tight boundaries of efficiency traditionally associated with scripts. Inthe present work, we describe how an efficient C11crystallographic library such as Clipper can bewrapped, adapted and generalized for use in both crystallographic and electron cryo-microscopyapplications, scripted with the Python language. We shall also place an emphasis on best practices inautomation, illustrating how this can be achieved with this new Python module.},
  file      = {:/Users/blaine/0papersLabeled/McNicholas2018AutomatingTasksInProteinStructureDeterminationWithTheClipperPythonModule.pdf:PDF},
  journal   = {Protein Science},
  keywords  = {automation; pipelines; scripting; Clipper; libraries; Python; SWIG; crystallography;electron cryo-microscopy; NumPy},
  publisher = {Wiley Online Library},
  year      = {2018},
}

